generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Aluno {
  id        Int      @id @default(autoincrement())
  nome      String
  idade     Int
  boletim   Boletim[]
}

model Professor {
  id         Int      @id @default(autoincrement())
  nome       String
  disciplina String
}

model Boletim {
  id          Int      @id @default(autoincrement())
  alunoId     Int      @unique
  logica      Float
  javascript  Float
  typescript  Float
  node        Float
  angular     Float
  react       Float
  aluno       Aluno    @relation(fields: [alunoId], references: [id])
}